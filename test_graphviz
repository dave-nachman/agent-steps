// create_recipe
digraph {
	compound=true
	rankdir=TB
	subgraph cluster_create_recipe {
		__phantom_create_recipe [label="" height=0 shape=none width=0]
		color=lightgrey label=create_recipe style="rounded,filled"
		__phantom_create_recipe -> draft_recipe [style=invis]
		draft_recipe [label=draft_recipe shape=box style="rounded,filled"]
		improve_recipe [label=improve_recipe shape=box style="rounded,filled"]
		validate_recipe [label=validate_recipe shape=box style="rounded,filled"]
		draft_recipe -> improve_recipe [style=""]
		improve_recipe -> validate_recipe [style=""]
		validate_recipe -> __phantom_create_recipe
	}
}
